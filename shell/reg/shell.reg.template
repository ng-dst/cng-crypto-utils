"subcommands"=""

[HKEY_CLASSES_ROOT\*\shell\Cryptography\shell\a_Encryption\shell]

[HKEY_CLASSES_ROOT\*\shell\Cryptography\shell\a_Encryption\shell\a_Encrypt]
@="Encrypt..."
"CommandFlags"=dword:00000040

[HKEY_CLASSES_ROOT\*\shell\Cryptography\shell\a_Encryption\shell\a_Encrypt\command]
